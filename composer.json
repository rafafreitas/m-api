{

    "scripts": {
        "sql:validate": "vendor/bin/doctrine orm:validate-schema",
        "sql:submit": "vendor/bin/doctrine orm:schema-tool:update --force",
        "sql": "vendor/bin/doctrine orm:schema-tool:update --dump-sql"
    },
    "require": {
        "slim/slim": "3.12",
        "phpmailer/phpmailer": "^6.0",
        "doctrine/orm": "^2.7.1",
        "firebase/php-jwt": "^5.0",
        "ext-curl": "*",
        "ext-json": "*",
        "beberlei/doctrineextensions": "^1.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "require-dev": {
        "sngrl/php-firebase-cloud-messaging": "dev-master"
    }
}
